    Описание:    Выходные данные гидростатического датчика уровня
==============================================================================*)
TYPE  ST_HydroLevel_Data :
STRUCT
    (* Данные об уровне *)
    rLevelMM: REAL;               (* Уровень в мм *)
    rLevelPercent: REAL;          (* Уровень в процентах от максимального *)
    
    (* Статус датчика *)
    eStatus: E_Inputs_Status; (* Статус датчика *)
    wErrorCode: WORD;             (* Код ошибки, битовая маска *)
    bValid: BOOL;                 (* Флаг достоверности данных *)
    
    (* Дополнительные диагностические данные *)
    rRawPressurePa: REAL;         (* Исходное значение давления *)
END_STRUCT
END_TYPE