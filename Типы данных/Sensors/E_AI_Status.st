==============================================================================*)
TYPE E_AI_Status :
(
    STATUS_OK,               		 	(* Нормальная работа датчика *)
    STATUS_VALUE_ABOVE_MAX,       	    (* Превышение максимально допустимого значения *)
    STATUS_VALUE_ABOVE_MIN,      		(* Значение ниже минимально допустимого значения *)
    STATUS_AI_ABOVE_RANGE,    		    (* Превышение диапазона аналогового входа *)
    STATUS_AI_BELOW_RANGE,   		    (* Значение ниже диапазона аналогового входа *)
    STATUS_WIRE_BREAK,       			(* Обрыв линии *)
    STATUS_CURRENT_OVERLOAD,  	        (* Перегрузка в канале измерения тока *)
    STATUS_NOT_READY,         			(* Измерение не готово *)
    STATUS_CONFIG_ERROR,     			(* Ошибка конфигурации *)
    STATUS_UNKNOWN            			(* Неизвестная ошибка *)
);
END_TYPE