    Описание:    Перечисление режимов мигания сигнальной лампы
==============================================================================*)
TYPE E_LampBlinkMode :
(
    LAMP_OFF := 0,            (* Лампа выключена *)
    LAMP_ON := 1,             (* Лампа постоянно включена - ошибка оборудования *)
    LAMP_SLOW := 2,           (* Медленное мигание - ошибка связи *)
    LAMP_FAST := 3            (* Быстрое мигание - ошибка измерения *)
);
END_TYPE